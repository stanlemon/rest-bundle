{
    "name": "stanlemon/rest-bundle",
    "description": "Create REST endpoints for Doctrine entities",
    "license": "MIT",
    "authors": [
        {
            "name": "Stan Lemon",
            "email": "stosh1985@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/stanlemon/DoctrineMongoDBBundle"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "psr/log": "~1.0",
        "symfony/framework-bundle": "~2.3|~3.0",
        "symfony/dependency-injection": "~2.3|~3.0",
        "symfony/yaml": "~2.3|~3.0",
        "symfony/finder": "~2.3|~3.0",
        "symfony/validator": "~2.3|~3.0",
        "doctrine/common": "~2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "jms/serializer": ">=0.14 <=1.1",
        "jms/serializer-bundle": ">=0.13 <=1.1",
        "willdurand/negotiation": ">=1.3.2 <2.0"
    },
    "require-dev": {
        "symfony/monolog-bundle": "~2.3",
        "symfony/form": "~2.3|~3.0",
        "symfony/browser-kit": "~2.3|~3.0",
        "doctrine/orm": "~2.3",
        "mongofill/mongofill": "dev-master@dev",
        "doctrine/mongodb": "~1.2",
        "doctrine/mongodb-odm": "~1.0",
        "doctrine/mongodb-odm-bundle": "dev-symfony-3@dev",
        "phpunit/phpunit": "5.7"
    },
    "autoload": {
        "psr-4": {
            "Lemon\\RestBundle\\": "./"
        }
    }
}
